@model Auth.Models.Results

@{
    ViewBag.Title = "Result";
    var currentUser = Session["CurrentUser"] as Auth.Models.User;
    
}

<h2>Result</h2>
<div class="container">
    <table class="table table-dark">
        <tr>
            <th>TEst name</th>
            <th>TEst score</th>
        </tr>
        @foreach(var item in currentUser.Results)
        {
            <tr>
                <td>@item.Test.name</td>
                <td>@item.score</td>
            </tr>
        }
    </table>
</div>
